<h3 class="top_banner">Edit User Access</h3>
<div class="main-content">
  <div class="row">
    <div class="column">
      <table class="table">
        <tbody>
          <% @users.all.reject{|u| u == current_user}.each do |user| %>
            <tr>
              <%= render user %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
